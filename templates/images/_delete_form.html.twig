<form method="post" action="{{ path('app_images_delete', {'id': image.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ image.id) }}">
    <button class="btn"><img class="delete-btn" src="{{ asset('img/delete.svg') }}" ></button>
</form>
